from matplotlib import pyplot as plt

x = [5, 2, 9, 4, 7]
y = [10, 5, 8, 4, 2]

# Line Plot
plt.plot(x, y)
plt.title("Line Plot")
plt.show()

# Bar Plot
plt.bar(x, y)
plt.title("Bar Plot")
plt.show()

# Histogram
plt.hist(y)
plt.title("Histogram")
plt.show()

# Scatter Plot
plt.scatter(x, y)
plt.title("Scatter Plot")
plt.show()
